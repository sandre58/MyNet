<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:my="http://mynet.com/avalonia"
                    x:ClassModifier="internal">
    
    <!-- Opacities -->
    <x:Double x:Key="MyNet.Opacity.Secondary">0.7</x:Double>
    <x:Double x:Key="MyNet.Opacity.Tertiary">0.56</x:Double>
    <x:Double x:Key="MyNet.Opacity.Watermark">0.60</x:Double>
    <x:Double x:Key="MyNet.Opacity.Overlay">0.40</x:Double>
    <x:Double x:Key="MyNet.Opacity.Disabled">0.40</x:Double>
    <x:Double x:Key="MyNet.Opacity.Ripple">0.19</x:Double>
    <x:Double x:Key="MyNet.Opacity.Active">0.16</x:Double>
    <x:Double x:Key="MyNet.Opacity.Hover">0.13</x:Double>
    <x:Double x:Key="MyNet.Opacity.Filled">0.11</x:Double>
    <x:Double x:Key="MyNet.Opacity.Alternate">0.03</x:Double>
    
    <!-- Shadows -->
    <my:ShadowDepth x:Key="MyNet.ShadowDepth.Container">Depth1</my:ShadowDepth>
    <my:ShadowDepth x:Key="MyNet.ShadowDepth.Control">Depth2</my:ShadowDepth>
    <my:ShadowDepth x:Key="MyNet.ShadowDepth.Popup">Depth2</my:ShadowDepth>
    <my:ShadowDepth x:Key="MyNet.ShadowDepth.ToolTip">Depth2</my:ShadowDepth>
    
</ResourceDictionary>