<Styles x:Class="MyNet.Avalonia.Theme.MyTheme" xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Styles.Resources>
        <ResourceDictionary>
            
            <ResourceDictionary.ThemeDictionaries>
                <ResourceInclude x:Key="Default" Source="avares://MyNet.Avalonia.Theme/Themes/Light.axaml" />
                <ResourceInclude x:Key="Dark" Source="avares://MyNet.Avalonia.Theme/Themes/Dark.axaml" />
                <ResourceInclude x:Key="Light" Source="avares://MyNet.Avalonia.Theme/Themes/Light.axaml" />
            </ResourceDictionary.ThemeDictionaries>
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://MyNet.Avalonia.Theme/ResourceDictionaries/_index.axaml" />
                <ResourceInclude Source="avares://MyNet.Avalonia.Theme/Controls/_index.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Styles.Resources>

    <!-- Styles -->
    <StyleInclude Source="avares://MyNet.Avalonia.Theme/Styles/_index.axaml" />
</Styles>
