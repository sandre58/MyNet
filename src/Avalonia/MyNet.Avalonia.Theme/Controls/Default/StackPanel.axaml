<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ControlTheme x:Key="{x:Type StackPanel}" TargetType="{x:Type StackPanel}">
        <Setter Property="Focusable" Value="False" />
        <Style Selector="^.ExtraSmall">
            <Setter Property="Spacing" Value="{StaticResource MyNet.Spacing.ExtraSmall}" />
        </Style>
        <Style Selector="^.Small">
            <Setter Property="Spacing" Value="{StaticResource MyNet.Spacing.Small}" />
        </Style>
        <Style Selector="^.Medium">
            <Setter Property="Spacing" Value="{StaticResource MyNet.Spacing.Medium}" />
        </Style>
        <Style Selector="^.Large">
            <Setter Property="Spacing" Value="{StaticResource MyNet.Spacing.Large}" />
        </Style>
        <Style Selector="^.ExtraLarge">
            <Setter Property="Spacing" Value="{StaticResource MyNet.Spacing.ExtraLarge}" />
        </Style>
    </ControlTheme>
</ResourceDictionary>
