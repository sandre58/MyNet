<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:CompileBindings="True">
    <ControlTheme x:Key="{x:Type ThemeVariantScope}" TargetType="ThemeVariantScope">
        <Setter Property="TextElement.Foreground" Value="{StaticResource MyNet.Color.Application.Foreground}" />
        <Setter Property="TextElement.FontSize" Value="{StaticResource MyNet.Font.Size}" />
        <Setter Property="TextElement.FontFamily" Value="{StaticResource MyNet.Font.FontFamily}" />
    </ControlTheme>
</ResourceDictionary>
