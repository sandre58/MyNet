<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:my="http://mynet.com/avalonia"
                    x:ClassModifier="internal">
    
    <!-- Durations -->
    <x:TimeSpan x:Key="MyNet.Timespan.Glyph">0:0:0.1</x:TimeSpan>
    <x:TimeSpan x:Key="MyNet.Timespan.Opacity">0:0:0.15</x:TimeSpan>
    <x:TimeSpan x:Key="MyNet.Timespan.Switch">0:0:0.2</x:TimeSpan>
    <x:TimeSpan x:Key="MyNet.Timespan.Slide">0:0:0.3</x:TimeSpan>
    <x:TimeSpan x:Key="MyNet.Timespan.Shadow">0:0:0.3</x:TimeSpan>
    <x:TimeSpan x:Key="MyNet.Timespan.Underline">0:0:0.4</x:TimeSpan>

    <!-- Transition -->
    <SineEaseInOut x:Key="MyNet.Transition.SineEaseInOut.NavigationMenu" />
    <Easing x:Key="MyNet.Transition.Easing.SplitView">0.1,0.9,0.2,1.0</Easing>
</ResourceDictionary>